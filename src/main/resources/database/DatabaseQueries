ALTER TABLE sample.wooden_product ADD CONSTRAINT wooden_product_un UNIQUE (product_id);
ALTER TABLE sample.wooden_product ALTER COLUMN product_id SET NOT NULL;

ALTER TABLE sample.electronic_product ADD CONSTRAINT electronic_product_un UNIQUE (product_id);
ALTER TABLE sample.electronic_product ALTER COLUMN product_id SET NOT NULL;

ALTER TABLE sample.metal_product ADD CONSTRAINT metal_product_un UNIQUE (product_id);
ALTER TABLE sample.metal_product ALTER COLUMN product_id SET NOT NULL;


